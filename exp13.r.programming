data_vector <- c(10, 20, 30, 40, 50)
min_value <- min(data_vector)
max_value <- max(data_vector)
mean_value <- mean(data_vector)
minmax_normalized <- (data_vector - min_value) / (max_value - min_value)
cat("Minimum value:", min_value, "\n")
cat("Maximum value:", max_value, "\n")
cat("Mean value:", mean_value, "\n")
cat("Min-max normalized data:", minmax_normalized, "\n")
